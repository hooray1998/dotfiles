#!/bin/bash
runsh=init

kk()
{
if [ $# -eq 1 ]
then
	runsh=$1;
	source $runsh
	if [ $? -eq 0 ]
	then
		echo "刷新完成,按Enter继续";
		read enter;
		./$runsh
	else
		echo "有bug, 加油！！！"
	fi
else 
	if [ "$runsh" = "init" ]
	then
		echo "缺少sh文件名"
	else
	source $runsh
	if [ $? -eq 0 ]
	then
		echo "编译完成,按Enter继续";
		echo "刷新完成,按Enter继续";
		read enter;
		./$runsh
	else
		echo "有bug, 加油！！！"
	fi
	fi
fi
}
